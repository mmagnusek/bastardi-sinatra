namespace :db do
  task :load_config do
    require 'active_record'
    ActiveRecord::Base.establish_connection(
      :adapter => 'sqlite3',
      :database =>  'production.db'
    )
    class Projection < ActiveRecord::Base
      scope :current, lambda { { :conditions => ['date == ?', Date.today] } }
    end
  end
  
